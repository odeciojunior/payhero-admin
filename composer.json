{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "arcanedev/log-viewer": "9.0.0",
        "automattic/woocommerce": "^3.0",
        "barryvdh/laravel-cors": "3.0.0",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "cloudflare/sdk": "1.3.0",
        "doctrine/dbal": "3.4.2",
        "fakerphp/faker": "^1.20",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^6.3",
        "google/apiclient": "^2.10",
        "guzzlehttp/guzzle": "^7.4.5",
        "guzzlehttp/psr7": "2.4.5",
        "intervention/image": "^2.4",
        "jenssegers/agent": "2.6.3",
        "laracasts/flash": "^3.0",
        "laracasts/presenter": "^0.2.1",
        "laracasts/utilities": "3.*",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "5.9.9",
        "laravel/legacy-factories": "^1.3",
        "laravel/passport": "10.4.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "3.4.6",
        "laravelcollective/html": "6.*",
        "lcobucci/jwt": "4.1.5",
        "league/flysystem": "3.2.1",
        "league/flysystem-aws-s3-v3": "3.1.1",
        "maatwebsite/excel": "^3.1",
        "nwidart/laravel-modules": "^8.0",
        "paquettg/php-html-parser": "^2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "7.0.2",
        "qcod/laravel-settings": "1.*",
        "sendgrid/sendgrid": "~7",
        "sentry/sentry-laravel": "^4.9",
        "spatie/laravel-activitylog": "4.5.3",
        "spatie/laravel-health": "^1.14",
        "spatie/laravel-permission": "5.5.5",
        "spatie/laravel-schedule-monitor": "3.0.3",
        "torann/geoip": "^1.0",
        "vinkla/hashids": "10.0.1"
    },
    "require-dev": {
        "ext-curl": "*",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.8.2",
        "beyondcode/laravel-query-detector": "^1.4",
        "filp/whoops": "~2.0",
        "kitloong/laravel-migrations-generator": "^6.10",
        "krlove/eloquent-model-generator": "dev-master",
        "laravel/pint": "^1.17",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.0",
        "shuchkin/simplexlsx": "^0.8.13",
        "spatie/laravel-ignition": "^1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "symfony/thanks": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": []
}
