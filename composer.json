{
    "name"             : "laravel/laravel",
    "description"      : "The Laravel Framework.",
    "keywords"         : [
        "framework",
        "laravel"
    ],
    "license"          : "MIT",
    "type"             : "project",
    "require"          : {
        "php": "^7.1.3",
        "ext-json": "^1.6",
        "barryvdh/laravel-cors": "0.11.3",
        "cloudflare/sdk": "^1.0",
        "ebanx/ebanx": "dev-master",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "6.3",
        "guzzlehttp/psr7": "1.5.2",
        "intervention/image": "^2.4",
        "jenssegers/agent": "2.6.3",
        "laracasts/flash": "^3.0",
        "laracasts/presenter": "^0.2.1",
        "laracasts/utilities": "3.0",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.4",
        "laravel/passport": "7.2.0",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nwidart/laravel-modules": "^5",
        "pagarme/pagarme-php": "4.0.0-alpha",
        "paquettg/php-html-parser": "^2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "3.3.1",
        "sendgrid/sendgrid": "~7",
        "sentry/sentry-laravel": "1.1.0",
        "slince/shopify-api-php": "2.2.0",
        "socialiteproviders/shopify": "^1.0",
        "spatie/laravel-permission": "^2.20",
        "vinkla/hashids": "^5",
        "vitorccsiqueira/zenvia-php": "2.1",
        "yajra/laravel-datatables": "^1.2"
    },
    "require-dev"      : {
        "barryvdh/laravel-debugbar"      : "^3.2",
        "filp/whoops"                    : "^2.0",
        "fzaninotto/faker"               : "^1.4",
        "mockery/mockery"                : "^1.0",
        "nunomaduro/collision"           : "^2.0",
        "phpunit/phpunit"                : "^7.0",
        "symfony/thanks"                 : "^1.0",
        "krlove/eloquent-model-generator": "dev-master",
        "ext-curl": "*"
    },
    "autoload"         : {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4"   : {
            "App\\": "app/"
        }
    },
    "autoload-dev"     : {
        "psr-4": {
            "App\\"    : "app/",
            "Modules\\": "Modules/"
        }
    },
    "extra"            : {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts"          : {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd"  : [
            "@php artisan key:generate"
        ],
        "post-autoload-dump"       : [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:clear"
        ]
    },
    "config"           : {
        "preferred-install"  : "dist",
        "sort-packages"      : true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true
}
