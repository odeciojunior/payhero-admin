{
    "name"             : "laravel/laravel",
    "description"      : "The Laravel Framework.",
    "keywords"         : [
        "framework",
        "laravel"
    ],
    "license"          : "MIT",
    "type"             : "project",
    "require"          : {
        "php"                       : "^7.1.3",
        "ext-json"                  : "^1.6",
        "barryvdh/laravel-cors"     : "2.0.0",
        "cloudflare/sdk"            : "^1.0",
        "doctrine/dbal"             : "^2.10",
        "ebanx/ebanx"               : "dev-master",
        "fideloper/proxy"           : "^4.0",
        "guzzlehttp/guzzle"         : "6.*",
        "guzzlehttp/psr7"           : "1.6.1",
        "intervention/image"        : "^2.4",
        "jenssegers/agent"          : "2.6.3",
        "laravel/socialite"         : "^4.2",
        "laracasts/flash"           : "^3.0",
        "laravel/passport"          : "9.2.0",
        "laracasts/presenter"       : "^0.2.1",
        "laracasts/utilities"       : "3.*",
        "laravel/framework"         : "^7.0",
        "laravel/helpers"           : "^1.2",
        "laravel/horizon"           : "4.*",
        "laravel/tinker"            : "^2.0",
        "laravel/ui"                : "^2.0",
        "laravelcollective/html"    : "6.*",
        "league/flysystem"          : "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nwidart/laravel-modules"   : "^5",
        "pagarme/pagarme-php"       : "4.0.0-alpha",
        "paquettg/php-html-parser"  : "^2.0",
        "predis/predis"             : "^1.1",
        "pusher/pusher-php-server"  : "3.3.1",
        "qcod/laravel-settings"     : "^1.0",
        "sendgrid/sendgrid"         : "~7",
        "sentry/sentry-laravel"     : "^1.6.0",
        "slince/shopify-api-php"    : "2.5.0",
        "socialiteproviders/shopify": "^1.0",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-permission" : "3.11.0",
        "vinkla/hashids"            : "8.*",
        "vitorccsiqueira/zenvia-php": "2.1",
        "yajra/laravel-datatables"  : "^1.2",
        "torann/geoip"              : "^1.0"    
    },
    "require-dev"      : {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "~2.0",
        "krlove/eloquent-model-generator": "dev-master",
        "shuchkin/simplexlsx": "^0.8.13",
        "symfony/thanks": "^1.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "ext-curl": "*"    
    },
    "autoload"         : {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4"   : {
            "App\\": "app/"
        }
    },
    "autoload-dev"     : {
        "psr-4": {
            "App\\"    : "app/",
            "Modules\\": "Modules/"
        }
    },
    "extra"            : {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts"          : {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd"  : [
            "@php artisan key:generate"
        ],
        "post-autoload-dump"       : [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:clear"
        ]
    },
    "config"           : {
        "preferred-install"  : "dist",
        "sort-packages"      : true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true
}
