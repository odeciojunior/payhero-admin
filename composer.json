{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "arcanedev/log-viewer": "8.*",
    "automattic/woocommerce": "^3.0",
    "barryvdh/laravel-cors": "2.0.0",
    "barryvdh/laravel-dompdf": "^0.9.0",
    "cloudflare/sdk": "^1.1.6",
    "doctrine/dbal": "^2.10",
    "fakerphp/faker": "^1.14",
    "fideloper/proxy": "^4.0",
    "google/apiclient": "^2.10",
    "guzzlehttp/guzzle": "^6.2.2",
    "guzzlehttp/psr7": "1.8.2",
    "intervention/image": "^2.4",
    "jenssegers/agent": "^2.6",
    "laracasts/flash": "^3.0",
    "laracasts/presenter": "^0.2.1",
    "laracasts/utilities": "3.*",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1.2",
    "laravel/horizon": "5.*",
    "laravel/legacy-factories": "^1.1",
    "laravel/passport": "10.1.3",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "laravelcollective/html": "6.*",
    "lcobucci/jwt": "4.1.4",
    "league/flysystem": "^1.0",
    "league/flysystem-aws-s3-v3": "1.0.29",
    "maatwebsite/excel": "^3.1",
    "nwidart/laravel-modules": "^8.0",
    "paquettg/php-html-parser": "^2.0",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "3.3.1",
    "qcod/laravel-settings": "1.*",
    "sendgrid/sendgrid": "~7",
    "sentry/sentry-laravel": "2.5.3",
    "slince/shopify-api-php": "^2.5.1",
    "socialiteproviders/shopify": "^1.0",
    "spatie/laravel-activitylog": "^3.9",
    "spatie/laravel-permission": "4.0.1",
    "spatie/laravel-schedule-monitor": "^2.4",
    "torann/geoip": "^1.0",
    "vinkla/hashids": "9.*"
  },
  "require-dev": {
    "ext-curl": "*",
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.8.2",
    "beyondcode/laravel-query-detector": "^1.4",
    "facade/ignition": "^2.3.6",
    "filp/whoops": "~2.0",
    "krlove/eloquent-model-generator": "dev-master",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0",
    "shuchkin/simplexlsx": "^0.8.13",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    },
    "classmap": [
      "tests"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover",
      "@php artisan config:clear",
      "@php artisan view:clear",
      "@php artisan route:clear"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "symfony/thanks": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
  ]
}
